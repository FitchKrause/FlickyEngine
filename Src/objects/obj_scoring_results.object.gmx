        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Finish counting bonuses.

    if(CanCount == 0){
       if(BonusRings == 0 &amp;&amp; BonusTime == 0){
          if(CanDo == 0){
             CountSound = 0;
             PlaySound(snd_scoring_result, global.SFXVolume, 1, 1)
             global.Rings = 0;
             CanDo = 1;
          }
          if(CanDo = 1 &amp;&amp; CheckSound(snd_scoring_result) == false){
             CanDo = 2;
          }
          if(GoingOut = 0){
             if(CheckSound(obj_audio_manager.Jingle_ActClear) == false){
                GoingOut = 1
                alarm[4] = 100;
             }
          }
       }
    }

</string>
          </argument>
